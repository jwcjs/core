{
  "name": "jwcjs",
  "version": "0.0.0",
  "description": "Jwc.js is a JavaScript framework for using JSX to write web components on the web.",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "types": "./dist/**/*.d.ts",
  "files": [
