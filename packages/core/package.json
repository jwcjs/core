{
  "name": "jwcjs",
  "version": "0.1.0-alpha.0",
  "description": "Jwc.js is a JavaScript framework for using JSX to write web components on the web.",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jwcjs/core.git"
  },
  "homepage": "https://jwc.js.org",
