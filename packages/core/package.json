    "rollup-plugin-esbuild": "^5.0.0",
    "vite": "^4.0.1",
    "@jwcjs/reactively": "workspace:^1.0.0",
    "@jwcjs/runtime": "workspace:^1.0.0",
    "@jwcjs/shared": "workspace:^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rollup-plugin-ts": "^3.0.2"
